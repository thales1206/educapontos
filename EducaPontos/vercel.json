{
    "builds": [
        {
            "src": "EducaPontos/wsgi.py",
            "use": "@vercel/python",
            "config": { "maxLambdaSize": "15mb", "runtime": "python3.12"}
        },
        {
            "src": "build.sh",
            "use": "@vercel/static-build",
            "config": {
              "distDir": "staticfiles_build"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "EducaPontos/wsgi.py"
        },
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        }
    ]
}